<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
<persistence-unit name="SMSProgJPA">
	<class>SMSJPA.Negocio.Turno.turno</class>
	<class>SMSJPA.Negocio.Materia.materia</class>
	<class>SMSJPA.Negocio.Profesor.profesor</class>
	<class>SMSJPA.Negocio.Profesor.ProfesorAuxiliar</class>
	<class>SMSJPA.Negocio.Profesor.ProfesorResidente</class>
	<class>SMSJPA.Negocio.Profesor.ProfMateria</class>
	<class>SMSJPA.Negocio.Profesor.ClavesProfesorMateria</class>
		<validation-mode>NONE</validation-mode>
<properties>
	<property name="eclipselink.cache.shared.default" value="false"/>
	<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
	<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost/smsbbddjpa"/>
	<property name="javax.persistence.jdbc.user" value="root"/>
	<property name="javax.persistence.jdbc.password" value="1Manager"/>
	<!-- enable this property to see SQL and other logging -->
	<!-- property name="eclipselink.logging.level" value="FINE"/ -->
	<!--  <property name="eclipselink.ddl-generation" value="drop-and-create-tables"/>-->
	<property name="eclipselink.create-ddl-jdbc-file-name" value="createDDL_ddlGeneration.jdbc"/>
	<property name="eclipselink.drop-ddl-jdbc-file-name" value="dropDDL_ddlGeneration.jdbc"/>
	<property name="eclipselink.ddl-generation.output-mode" value="both"/>   
	
	
</properties>
</persistence-unit>
</persistence>
